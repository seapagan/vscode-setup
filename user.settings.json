{
  "CodeGPT.model": "gpt-4",
  "[css]": {
    "editor.colorDecorators": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[django-html]": {
    "editor.bracketPairColorization.enabled": false,
    "editor.defaultFormatter": "HookyQR.beautify",
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features",
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[javascriptreact]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[jinja-html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.markdownlint": true
    },
    "editor.quickSuggestions": {
      "comments": "on",
      "other": "on",
      "strings": "on"
    }
  },
  "[php]": {
    "editor.defaultFormatter": "junstyle.php-cs-fixer"
  },
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    },
    "editor.defaultFormatter": "ms-python.python",
    "editor.tabSize": 4
  },
  "[ruby]": {
    "editor.defaultFormatter": "misogi.ruby-rubocop",
    "editor.tabSize": 2
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
  },
  "[typescript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.tabSize": 2
  },
  "[yml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.tabSize": 2
  },
  "arduino.useArduinoCli": true,
  "beautify.language": {
    "html": [
      "django-html"
    ]
  },
  "code-runner.clearPreviousOutput": true,
  "code-runner.runInTerminal": true,
  "code-runner.saveFileBeforeRun": true,
  "codeQL.telemetry.enableTelemetry": true,
  "comment-divider.shouldLengthIncludeIndent": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "docker.images.sortBy": "CreatedTime",
  "editor.acceptSuggestionOnEnter": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeLensFontFamily": "Jetbrains Mono",
  "editor.defaultFormatter": null,
  "editor.folding": false,
  "editor.fontFamily": "Jetbrains Mono, Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "editor.fontWeight": "normal",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.guides.bracketPairs": true,
  "editor.guides.bracketPairsHorizontal": true,
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.mouseWheelZoom": true,
  "editor.parameterHints.enabled": false,
  "editor.rulers": [
    80,
    120
  ],
  "editor.snippetSuggestions": "top",
  "editor.stickyScroll.enabled": false,
  "editor.suggest.localityBonus": true,
  "editor.suggest.preview": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "emmet.excludeLanguages": [
    "markdown"
  ],
  "emmet.showAbbreviationSuggestions": true,
  "emmet.showExpandedAbbreviation": "always",
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "errorLens.fontStyleItalic": true,
  "errorLens.gutterIconSet": "defaultOutline",
  "errorLens.gutterIconsEnabled": true,
  "explorer.autoReveal": "focusNoScroll",
  "explorer.compactFolders": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.css": "css",
    "*.html.erb": "erb",
    "*.jinja": "jinja",
    "*.jsx": "javascriptreact",
    "*.php": "php",
    ".coveragerc": "ini",
    ".env.local": "jinja-properties",
    ".htmlvalidateignore": "gitignore",
    "LICENSE": "plaintext"
  },
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": false
  },
  "files.insertFinalNewline": true,
  "files.maxMemoryForLargeFilesMB": 8192,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.fetchOnPull": true,
  "git.showPushSuccessNotification": true,
  "github.copilot.advanced": {
    "indentationMode": {
      "*": true,
      "go": false,
      "javascript": true,
      "javascriptreact": true,
      "jsx": true,
      "python": true,
      "ruby": true,
      "typescript": true,
      "typescriptreact": true
    }
  },
  "github.copilot.enable": {
    "*": true,
    "css": false,
    "erb": false,
    "javascript": true,
    "javascriptreact": true,
    "markdown": true,
    "php": false,
    "plaintext": false,
    "python": true,
    "ruby": true,
    "scminput": false,
    "scss": true,
    "sql": true,
    "typescript": true,
    "yaml": true
  },
  "github.copilot.inlineSuggest.enable": true,
  "githubPullRequests.createOnPublishBranch": "never",
  "githubPullRequests.pullBranch": "never",
  "gitlens.advanced.messages": {
    "suppressCreatePullRequestPrompt": true
  },
  "gitlens.codeLens.scopes": [
    "document",
    "containers",
    "blocks"
  ],
  "gitlens.gitCommands.skipConfirmations": [
    "fetch:command",
    "stash-push:command",
    "switch:command",
    "pull:command"
  ],
  "gitlens.hovers.currentLine.over": "line",
  "highlight-matching-tag.highlightFromContent": true,
  "highlight-matching-tag.highlightSelfClosing": true,
  "html.format.endWithNewline": true,
  "html.format.indentHandlebars": true,
  "html.format.indentInnerHtml": true,
  "html.format.templating": true,
  "html.format.wrapLineLength": 80,
  "isort.importStrategy": "fromEnvironment",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "license.author": "Grant Ramsay",
  "license.default": "mit",
  "license.extension": ".txt",
  "license.year": "auto",
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "markdown-preview-enhanced.previewTheme": "github-light.css",
  "markdown-preview-github-styles.colorTheme": "system",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "right"
  },
  "notebook.output.textLineLimit": 500,
  "notebook.output.wordWrap": true,
  "open-in-browser.default": "firefox",
  "php.suggest.basic": true,
  "php.validate.enable": true,
  "prettier.proseWrap": "always",
  "prettier.quoteProps": "preserve",
  "python.defaultInterpreterPath": ".venv/bin/python",
  "python.interpreter.infoVisibility": "always",
  "python.pythonPath": "",
  "python.showStartPage": false,
  "redhat.telemetry.enabled": false,
  "remote.SSH.configFile": "",
  "remote.SSH.defaultExtensions": [
    "gitpod.gitpod-remote-ssh"
  ],
  "remote.SSH.remotePlatform": {},
  "rewrap.autoWrap.enabled": true,
  "rewrap.wrappingColumn": 80,
  "sonarlint.rules": {
    "javascript:S6480": {
      "level": "off"
    }
  },
  "sql-formatter.uppercase": true,
  "stylelint.packageManager": "yarn",
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.defaultProfile.windows": "Command Prompt",
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.shellIntegration.enabled": true,
  "testExplorer.useNativeTesting": false,
  "vs-kubernetes": {
    "vs-kubernetes.minikube-show-information-expiration": "2023-11-04T10:17:10.707Z",
    "vscode-kubernetes.helm-path.linux": "/home/seapagan/.vs-kubernetes/tools/helm/linux-amd64/helm",
    "vscode-kubernetes.kubectl-path.linux": "/home/seapagan/.vs-kubernetes/tools/kubectl/kubectl",
    "vscode-kubernetes.kubectl-path.windows": "C:\\Users\\sea_p\\.vs-kubernetes\\tools\\kubectl\\kubectl.exe",
    "vscode-kubernetes.minikube-path.linux": "/home/seapagan/.vs-kubernetes/tools/minikube/linux-amd64/minikube"
  },
  "vsintellicode.modify.editor.suggestSelection": "choseToUpdateConfiguration",
  "window.commandCenter": true,
  "window.restoreWindows": "none",
  "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}${separator}${appName}",
  "window.zoomLevel": 1,
  "workbench.colorTheme": "Cobalt2",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.productIconTheme": "material-product-icons",
  "workbench.startupEditor": "none"
}
