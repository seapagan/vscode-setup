{
  "[css]": {
    "editor.colorDecorators": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[django-html]": {
    "editor.defaultFormatter": "HookyQR.beautify",
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.tabSize": 4
  },
  "[yml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "beautify.language": {
    "html": ["htm", "html", "django-html"]
  },
  "code-runner.clearPreviousOutput": true,
  "code-runner.runInTerminal": true,
  "code-runner.saveFileBeforeRun": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "docker.images.sortBy": "CreatedTime",
  "editor.codeActionsOnSave": {
    "source.fixAll.markdownlint": true
  },
  "editor.defaultFormatter": null,
  "editor.folding": false,
  "editor.fontFamily": "JetBrains Mono, Consolas, 'Courier New', monospace",
  "editor.fontSize": 13,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.linkedEditing": true,
  "editor.rulers": [80, 120],
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "explorer.compactFolders": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.css": "css"
  },
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": false
  },
  "files.insertFinalNewline": true,
  "files.maxMemoryForLargeFilesMB": 8192,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.fetchOnPull": true,
  "gitlens.advanced.messages": {
    "suppressCreatePullRequestPrompt": true
  },
  "gitlens.codeLens.scopes": ["document", "containers", "blocks"],
  "gitlens.hovers.currentLine.over": "line",
  "html.format.endWithNewline": true,
  "html.format.indentHandlebars": true,
  "html.format.indentInnerHtml": true,
  "html.format.templating": true,
  "html.format.wrapLineLength": 80,
  "liveServer.settings.donotShowInfoMsg": true,
  "prettier.proseWrap": "always",
  "python.showStartPage": false,
  "redhat.telemetry.enabled": false,
  "rewrap.autoWrap.enabled": true,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.defaultProfile.windows": "Command Prompt",
  "terminal.integrated.fontSize": 13,
  "vs-kubernetes": {
    "vscode-kubernetes.helm-path.linux": "/home/seapagan/.vs-kubernetes/tools/helm/linux-amd64/helm",
    "vscode-kubernetes.kubectl-path.linux": "/home/seapagan/.vs-kubernetes/tools/kubectl/kubectl",
    "vscode-kubernetes.minikube-path.linux": "/home/seapagan/.vs-kubernetes/tools/minikube/linux-amd64/minikube"
  },
  "vsicons.dontShowNewVersionMessage": true,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.zoomLevel": 0,
  "workbench.colorTheme": "GitHub Dark",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.productIconTheme": "material-product-icons",
  "workbench.startupEditor": "none"
}
